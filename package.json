{
  "name": "wenyan-lang",
  "displayName": "ÊñáË®Ä Wenyan Lang",
  "description": "ÊñáË®Ä Wenyan Lang for VSCode",
  "version": "0.2.0",
  "publisher": "antfu",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:wenyan"
  ],
  "keywords": [
    "wenyan",
    "wenyan-lang"
  ],
  "icon": "res/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/wenyan-lang-vscode"
  },
  "homepage": "https://github.com/antfu/wenyan-lang-vscode",
  "bugs": "https://github.com/antfu/wenyan-lang-vscode/issues",
  "main": "./dist/extension",
  "contributes": {
    "languages": [
      {
        "id": "wenyan",
        "aliases": [
          "ÊñáË®Ä"
        ],
        "extensions": [
          ".wy"
        ]
      }
    ],
    "snippets": [
      {
        "language": "wenyan",
        "path": "./snippets/static.json"
      }
    ],
    "grammars": [
      {
        "language": "wenyan",
        "scopeName": "source.wenyan",
        "path": "./syntaxes/wenyan.tmGrammar.json"
      }
    ]
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "‚ö° Features"
      },
      {
        "type": "fix",
        "section": "üêû Bug Fixes"
      },
      {
        "type": "refactor",
        "section": "üîÆ Refactor"
      },
      {
        "type": "test",
        "section": "‚öó Tests"
      },
      {
        "type": "docs",
        "section": "üìö Documentations"
      },
      {
        "type": "i18n",
        "section": "üåç Internationalization"
      },
      {
        "type": "ci",
        "hidden": true
      }
    ]
  },
  "scripts": {
    "publish:minor": "standard-version --release-as minor && git push --follow-tags && vsce publish && npm run gh-release",
    "publish:patch": "standard-version --release-as patch && git push --follow-tags && vsce publish && npm run gh-release",
    "gh-release": "conventional-github-releaser -p angular",
    "vscode:prepublish": "webpack --mode development",
    "compile": "webpack --mode development",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "watch": "webpack --mode development --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "0.2.10",
    "@types/jest": "^24.0.23",
    "@types/node": "12.12.17",
    "@types/vscode": "^1.40.0",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "conventional-github-releaser": "3.1.3",
    "eslint": "6.7.2",
    "jest": "^24.9.0",
    "rimraf": "3.0.0",
    "standard-version": "7.0.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "6.2.1",
    "typescript": "^3.7.3",
    "webpack": "4.41.3",
    "webpack-cli": "3.3.10",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "dependencies": {}
}
